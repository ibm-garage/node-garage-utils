{
  "name": "garage-utils",
  "version": "3.2.0",
  "description": "Common utilities for Node/Express applications at the IBM Cloud Garage.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm-garage/node-garage-utils.git"
  },
  "author": "Dave Steinberg <davidms@ca.ibm.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all lint test:unit:cover",
    "test:unit": "mocha test/**/*.js",
    "test:unit:watch": "mocha --watch test/**/*.js",
    "test:unit:cover": "istanbul cover _mocha test/**/*.js",
    "lint": "eslint index.js lib test",
    "format": "prettier --write '**/*.@(js|json)'",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=3.5.3"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-prettystream-circularsafe": "^0.3.1",
    "cfenv": "^1.1.0",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^5.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.6",
    "sinon": "^6.0.1",
    "standard-version": "^4.4.0"
  }
}
