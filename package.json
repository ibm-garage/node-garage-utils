{
  "name": "garage-utils",
  "version": "4.3.0",
  "description": "Common utilities for Node/Express applications at the IBM Cloud Garage.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm-garage/node-garage-utils.git"
  },
  "author": "Dave Steinberg <davidms@ca.ibm.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "coverage": "cat coverage/lcov.info | coveralls",
    "lint": "eslint index.js lib bin",
    "format": "prettier --write '**/*.@(js|json)'",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "index.js",
    "lib/",
    "bin/"
  ],
  "bin": {
    "cfutil": "bin/cfutil.js"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-prettystream-circularsafe": "^0.3.1",
    "commander": "^5.0.0",
    "eol": "^0.9.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "standard-version": "^7.1.0"
  }
}
